        declarations {
            branch[TOC_Partes] {
                name = "Dados das Partes"
                index = true
                operations {
                    |mtxPartes|.ask()
                }
            },
            branch[TOC_Informacoes] {
                name = "Informações do Contrato"
                index = true
                operations {
                    <motivoInformacao>.ask(),
                    <tiposInformacao>.ask(),
                    <strMulta>.ask(),
                    <aliciamento>.ask(),
                    <strVigencia>.ask()
                }
            },
            branch[TOC_ForoAssinatura] {
                name = "Foro e Assinatura"
                index = true
                operations {
                    <strForo>.ask(),
                    <assinaturaDigital>.ask(),
                    <strLocalData>.ask(),
                    <strTestemunhas>.ask()
                }
            },
            branch[TOC_Encerramento] {
                name = "Encerramento"
                index = true
                operations {
                    <desejaEmail>.ask()
                }
            }
        }
        operations {
            use TOC_Partes,
            use TOC_Informacoes,
            use TOC_ForoAssinatura,
            use TOC_Encerramento
        }
    }
    extra {
        operations {
            if (<desejaEmail>) {
                foreach (<partes> IN |mtxPartes|) {
